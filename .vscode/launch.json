{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Cortex Debug",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceRoot}/quectel_build/release/target/QUEC_N32G430.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "jlink",
            "serverpath": "D:/JLink/JLinkGDBServerCL.exe", // 需要根据 jlink 安装目录替换 JLinkGDBServerCL.exe 所在路径
            "device": "N32G430C8",
            "interface": "swd",
            "armToolchainPath": "${workspaceRoot}/quectel_build/tools/gcc-arm-none-eabi-10.3-2021.10-win32/bin",
            "symbolFiles":[
                "${workspaceRoot}/quectel_build/release/target/QUEC_N32G430.elf",
            ],
            "preRestartCommands":[
                "monitor reset"
            ],
        }
    ]
}